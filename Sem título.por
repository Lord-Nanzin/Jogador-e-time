programa {
  funcao inicio() {
    inteiro idade, menoridade=0, maioridade=0

    para(inteiro time=1; time<4; time++){
      para(inteiro jogador=1; jogador<7; jogador++){
        escreva("Digite a idade do ", jogador,"jogador do ", time,"° time: ")
        leia(idade)
      se((time==1) e (jogador==1)){
        maioridade=idade
        menoridade=idade
        }
       }
      }
    }
  }
}
